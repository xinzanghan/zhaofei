page{
  background-color: #fff;
}
.head {
  width: 100%;
  height: 198rpx;
  background-color: #15A6E4;
  position: relative;
  z-index: -1;
}

.head_title {
  color: #ffff;
  padding-top: 24rpx;
  padding-left: 36rpx;
  font-size: 40rpx;
}

.wrap {
  width: 100%;
  box-sizing: border-box;
  /* height: 1392rpx; */
  /* top: 0; */
  margin-top: -70rpx;
  position: absolute;
  background-color: #ffff;
  border-radius: 40rpx;
}

.info_list {
  margin: 0 32rpx;
  height: 100%;
}

.info_item {
  width: 100%;
  box-sizing: border-box;
  height: 140rpx;
  text-align: center;
  line-height: 140rpx;
  background-color: #F7F8FA;
  margin-top: 20rpx;
  border-radius: 24rpx;
}

